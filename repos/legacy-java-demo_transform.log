import jakarta.persistence.*;  // Changed from javax.jdo
import jakarta.validation.constraints.*;

@Entity  // Changed from @PersistenceCapable
@Table(name = "customer")
public class Customer {
    
    @Id  // Changed from @PrimaryKey
    @Column(name = "id", nullable = false, length = 50)
    @NotNull
    @Size(min = 1, max = 50)
    @Pattern(regexp = "^[A-Z0-9-]+$")
